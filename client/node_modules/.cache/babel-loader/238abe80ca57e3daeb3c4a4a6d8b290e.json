,IAAA,MADA,oBACA;AACA,MAAA,YAAA,CAAA,KAAA;AACA,WAAA,OAAA,CAAA,IAAA,CAAA,QAAA;AACA;AAJA;AA7BA,CAAA","sourcesContent":["<script>\nimport axios from 'axios'\n\nconst baseUrl = 'http://localhost:5000/user/info'\n\nexport default {\n  name: 'User',\n\n  components: {\n        // mdbInput,\n    mdbContainer,\n    mdbRow,\n    mdbCol,\n        // mdbBtn\n  },\n\n  data() {\n    return {\n      name: '',\n      email: '',\n    }\n  },    \n  created() {\n    if (localStorage.getItem('token') === null) {\n      this.$router.push('/login')\n    }\n  },\n  mounted() {\n    axios.get(baseUrl, { headers: { 'access_token': localStorage.getItem('token')}})\n      .then(res => {\n        this.name = res.data.user.name\n        this.email = res.data.user.email\n      })\n  },\n  methods: {\n    logout() {\n      localStorage.clear();\n      this.$router.push('/login');\n    }\n  }\n}\n</script>\n\n\n<template>\n  <div>\n    <button @click=\"logout\">logout</button>\n    <h3>Name: {{ name }}</h3>\n    <h3>Email: {{ email }}</h3>\n  </div>\n</template>"],"sourceRoot":"src/components/auth"}]}