A,IAAA,GAAA,CAAA,CAAA,CAAA;AACA,KAPA;AASA,IAAA,WATA,uBASA,EATA,EASA;AAAA;AAAA;AAAA;AAAA;AAAA;AAAA;AAAA;AAAA;AAAA,uBACA,KAAA,CAAA,MAAA,wCAAA,EAAA,EADA;;AAAA;AAAA;AAAA;AAAA;AAAA;AAAA;AAAA;AAEA;AAXA,GA1BA;AAwCA,EAAA,QAAA,EAAA;AACA,IAAA,SADA,uBACA;AACA,aAAA,KAAA,MAAA,CAAA,KAAA,GAAA,IAAA,CAAA,UAAA,CAAA,EAAA,CAAA;AAAA,eAAA,IAAA,IAAA,CAAA,CAAA,CAAA,SAAA,IAAA,IAAA,IAAA,CAAA,CAAA,CAAA,SAAA,CAAA;AAAA,OAAA,CAAA;AACA;AAHA;AAxCA,CAAA","sourcesContent":["<script>\nimport axios from 'axios'\nimport { mdbContainer, mdbRow, mdbCol } from 'mdbvue';\nimport { formatDate } from '../../utils/formatDate'\n\nexport default {\n    name: \"Home\",\n    components: {\n        // mdbInput,\n        mdbContainer,\n        mdbRow,\n        mdbCol,\n        // mdbBtn\n    },\n\n    data() {\n        return {\n            models: [],\n            error: ''\n        }\n    },\n\n    async created() {\n        try {\n            const response = await axios.get('http://localhost:5000/models')\n            this.models = response.data\n        }catch(err) {\n            this.error = err\n        }\n    },\n\n    methods: {\n        details(id) {\n            this.$router.push({name: 'Details', params: {id: id}})\n        },\n        \n        formatDate(that) {\n            return formatDate(new Date(that - 1))\n        },\n\n        async deleteModel(id){\n            await axios.delete(`http://localhost:5000/models/${id}`)\n        },\n    },\n\n        computed: { \n            filtering()  {\n                return this.models.slice().sort((a,b) => new Date(b.createdAt) - new Date(a.createdAt))\n            }\n        }\n}\n</script>\n\n<template>\n    <mdb-container>\n            <mdb-row  class=\"justify-content-md-center\">\n                <mdb-col\n                    \n                    col=\"6\"\n                >\n                    <mdb-row>\n\n                        <mdb-col\n                            v-for=\"(model, index) in filtering\"\n                            :key=\"index\" \n                            class=\"post\" \n                            col='12'\n                        >\n                            <mdb-row>\n                                <mdb-col class=\"creation\">\n                                    <img class='img_creator' src=\"https://upload.wikimedia.org/wikipedia/commons/thumb/9/99/Unofficial_JavaScript_logo_2.svg/1200px-Unofficial_JavaScript_logo_2.svg.png\" alt=\"\">\n                                    <b>Creation</b> \n                                        <!-- {{formatDate(model.createdAt) }} -->\n                                        {{ formatDate(new Date(model.createdAt)) }}\n                                </mdb-col>\n                                <div class=\"w-100\"></div>\n                                <mdb-col>\n                                    <!-- {{ model.image }}    -->\n                                    <img \n                                        v-if='model.image != \"\"' \n                                        :src=\"`data:image/${'png' || 'jpeg' || 'jpg'};base64,${model.image}`\" \n                                        alt=\"\"\n                                    >\n                                </mdb-col>\n                                <div class=\"w-100\"></div>\n                                <mdb-col><h5 class=\"title\" @click=\"details(model._id)\">{{ model.title }}</h5></mdb-col>\n                                <div class=\"w-100\"></div>\n                                <mdb-col><p>{{ model.description }}</p></mdb-col>\n                                <!-- <button @click=\"deleteModel(model._id)\">s</button> -->\n                            </mdb-row>\n                            \n                        </mdb-col>\n                    </mdb-row>\n                </mdb-col>\n            </mdb-row>\n    </mdb-container>\n</template>\n\n\n<style lang=\"scss\" scoped>\n    .post{\n        background-color: white;\n        border-radius: 1.5px;\n        border: 1px solid rgb(207, 207, 207);\n        margin-top: 20px;\n    }    \n    .creation {\n        font-size: 13px;\n    }\n    b{\n        font-weight: 500;\n    }\n    .title{\n        font-weight: 600;\n    }\n    .img_creator{\n        width: 14px;\n        border-radius: 20px;\n        padding-bottom: 5px;\n        padding-right: 2px;\n    }\n        img{\n            width: 100%;\n        }\n\n</style>"],"sourceRoot":"src/components/main"}]}