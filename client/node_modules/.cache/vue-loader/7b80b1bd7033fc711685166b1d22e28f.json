    email: '',\n            password: '',\n            error: '',\n            showModal: false\n        }\n    },\n\n    methods: {\n        register() {\n            const newUser = {\n                name: this.name,\n                email: this.email,\n                password: this.password\n            }\n            \n            axios.post(baseUrl, newUser)\n                .then(res => {\n                    console.log(res)\n                    this.$router.push('/login')\n                }, err => {\n                    console.log(err.message)\n                })\n        }\n    }\n\n}\n\n\n</script>\n\n<template>\n  <!-- <mdb-container>\n    <section class=\"form-elegant\">\n      <mdb-row class=\"justify-content-md-center\">\n        <mdb-col md=\"5\">\n          <mdb-card>\n            <mdb-card-body class=\"mx-4\">\n              <div class=\"text-center\">\n                <h3 class=\"dark-grey-text mb-5\"><strong>Sign up</strong></h3>\n              </div>\n              <mdb-input label=\"Your name\" type=\"name\" v-model=\"name\" />\n              <mdb-input label=\"Your email\" type=\"email\" v-model=\"email\" />\n              <mdb-input label=\"Your password\" type=\"password\" containerClass=\"mb-0\" v-model=\"password\" />\n              <p class=\"font-small blue-text d-flex justify-content-end pb-3\">Forgot <a href=\"#\" class=\"blue-text ml-1\"> Password?</a></p>\n              <div class=\"text-center mb-3\">\n                <mdb-btn type=\"button\" gradient=\"blue\" rounded class=\"btn-block z-depth-1a\" @click=\"register\">Sign up</mdb-btn>\n              </div>\n              <p class=\"font-small dark-grey-text text-right d-flex justify-content-center mb-3 pt-2\"> or Sign in with:</p>\n              <div class=\"row my-3 d-flex justify-content-center\">\n                <mdb-btn type=\"button\" color=\"white\" rounded class=\"mr-md-3 z-depth-1a\"><mdb-icon fab icon=\"facebook\" class=\"blue-text text-center\"/></mdb-btn>\n          