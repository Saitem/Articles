ents: {\n    mdbContainer,\n    mdbRow,\n    mdbCol,\n    mdbInput,\n    mdbBtn\n  },\n\n  data() {\n    return {\n      name: '',\n      email: '',\n      userImage: userImage\n    }\n  },    \n  created() {\n    if (localStorage.getItem('token') === null) {\n      this.$router.push('/login')\n    }\n  },\n  mounted() {\n    axios.get(baseUrl, { headers: { 'access_token': localStorage.getItem('token')}})\n      .then(res => {\n        this.name = res.data.user.name\n        this.email = res.data.user.email\n      })\n  },\n  methods: {\n    logout() {\n      localStorage.clear();\n      this.$router.push('/login');\n    }\n  }\n}\n</script>\n\n\n<template>\n\n  <mdb-container>\n    <mdb-row  class=\"justify-content-md-center\">\n      <div class=\"text-center\">\n        <mdb-col col=\"12\">\n            <img :src=\"userImage\" class=\"img-fluid user_image\" alt=\"user_image\">\n        </mdb-col>\n        <mdb-col>\n          <mdb-input v-model=\"name\">{{ name }}</mdb-input>\n          <mdb-input v-model=\"email\">{{ email }}</mdb-input>\n        </mdb-col>  \n        <mdb-btn @click=\"logout\">Log out</mdb-btn>\n      </div>\n    </mdb-row>\n  </mdb-container>\n  <!-- <div>\n    <button @click=\"logout\">logout</button>\n    <h3>Name: {{ name }}</h3>\n    <h3>Email: {{ email }}</h3>\n  </div> -->\n</template>\n\n<style lang=\"scss\" scoped>\n  .user_image{\n    width: 45%;\n    text-align: center;\n    border-radius: 100%;\n    border: 3px solid grey;\n  }\n</style>"]}]}